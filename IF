#include <stdio.h>
#include <stdlib.h>

void main()
{
    int num1, num2;
    printf("ingrese dos numeros enteros\n");
    scanf("%i\n", &num1);
    scanf("%i", &num2);

    if (num1 > num2)
        {
            printf("el numero mayor es %i", num1);
        }
    else
        {
            printf("el numero mayor es %i\n", num2);
        }
    system("pause");
}
